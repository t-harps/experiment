<div class="row">
<div class="col-sm-1">
</div>
  <div class="col-sm-3">
    <h1> Users </h1>
    <% @users.each do |user| %>
      <h2> <%= "#{user.email} -- id: #{user.id}" %> </h2>
      <h3>Friends:</h3>
      <% user.friends.each do |friend| %>
        <p> <%= "#{friend.email} -- #{friend.id}" %> </p>
      <% end %>
      <h3>Developers:</h3>
      <% user.developers.each do |dev| %>
        <p> <%= "#{dev.email} -- #{dev.id}" %> </p>
      <% end %>
    <% end %>
  </div>

  <div class="col-sm-4">
    <h1> Developers </h1>
    <% @developers.each do |dev| %>
      <h2> <%= "#{dev.email} -- #{dev.id}" %> </h2>
      <h3> Users: </h3>
      <% dev.users.each do |user| %>
        <p> <%= "#{user.email} -- #{user.id}" %> </p>
      <% end %>
    <% end %>
  </div>

  <div class="col-sm-4">
    <h1> Links </h1>
    <% @links.each do |link| %>
      <p><%= "#{User.find(link.user_id).email} -- #{link.linkable_id}" %></p>
    <% end %>
  </div>

</div>